{
  "name": "History Search",
  "version": "0.1",
  "description": "An extension to show latest google queries.",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["storage", "history"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "manifest_version": 2
}